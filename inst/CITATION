citHeader("To cite CNVgears in publications use:")

citEntry(
  entry    = "Unpublished",
  title    = "CNVgears",
  author   = "Simone Montalbano",
  note     = "At the moment unpublished package available at Github.",
  url      = "https://github.com/SinomeM/CNVgears",
  textVersion = paste0("CNVgears, Simone Montalbano,",
                       " https://github.com/SinomeM/CNVgears, 2020"
  )
)
